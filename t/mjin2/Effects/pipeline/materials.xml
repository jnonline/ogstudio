<compositor>
    <!-- This simply uses the pipeline defaults -->
    <material name="plainSurface">
    </material>
    <material name="bumpySurface">
        <texture unit="1" varname="bumpMap">bump</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="crawlingPlainSurface">
        <uniform name="crawlDiffMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="crawlingBumpySurface">
        <texture unit="1" varname="bumpMap">bump</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
        <uniform name="crawlBumpMap" type="int">
            <value>1</value>
        </uniform>
        <uniform name="crawlDiffMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="bumpyGlowingSurface">
        <texture unit="1" varname="bumpMap">bump</texture>
        <texture unit="2" varname="glowMap">glow</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
        <uniform name="useGlowMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="bumpyGlowing2Surface">
        <texture unit="1" varname="bumpMap">bump</texture>
        <texture unit="2" varname="glowMap">glow2</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
        <uniform name="useGlowMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="cubemapped">
        <texture unit="1" varname="bumpMap">bump</texture>
        <texture unit="2" varname="glowMap">reflectionGlow</texture>
        <texture unit="4" varname="cubeMapMask">reflectionMask</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
        <uniform name="useCubeMap" type="int">
            <value>1</value>
        </uniform>
        <uniform name="useGlowMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="floorSurface">
        <texture unit="0" varname="diffMap">floorDiffuse</texture>
        <texture unit="1" varname="bumpMap">floorNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="wallSurface">
        <texture unit="0" varname="diffMap">wallDiffuse</texture>
        <texture unit="1" varname="bumpMap">wallNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="roofSurface">
        <texture unit="1" varname="bumpMap">roofNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="columnSurface">
        <texture unit="0" varname="diffMap">columnDiffuse</texture>
        <texture unit="1" varname="bumpMap">columnNormal</texture>
        <texture unit="5" varname="heightMap">columnHeight</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
        <uniform name="useHeightMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="pipeSurface">
        <texture unit="0" varname="diffMap">pipeDiffuse</texture>
        <texture unit="1" varname="bumpMap">pipeNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="pipeDiskSurface">
        <texture unit="0" varname="diffMap">pipeDiffuse</texture>
        <texture unit="1" varname="bumpMap">pipeDiskNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="railBaseSurface">
        <texture unit="0" varname="diffMap">columnDiffuse</texture>
        <texture unit="1" varname="bumpMap">railBaseNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="railSurface">
        <texture unit="0" varname="diffMap">railDiffuse</texture>
        <texture unit="1" varname="bumpMap">railNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="lineSurface">
        <texture unit="0" varname="diffMap">lineDiffuse</texture>
        <texture unit="1" varname="bumpMap">lineNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="lineBeltSurface">
        <texture unit="0" varname="diffMap">lineBeltDiffuse</texture>
        <texture unit="1" varname="bumpMap">lineBeltNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="processorSurface">
        <texture unit="0" varname="diffMap">columnDiffuse</texture>
        <texture unit="1" varname="bumpMap">processorNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="craneBaseSurface">
        <texture unit="0" varname="diffMap">columnDiffuse</texture>
        <texture unit="1" varname="bumpMap">craneBaseNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="craneArmsBaseSurface">
        <texture unit="0" varname="diffMap">columnDiffuse</texture>
        <texture unit="1" varname="bumpMap">craneArmsBaseNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="craneArmsRotSurface">
        <texture unit="0" varname="diffMap">columnDiffuse</texture>
        <texture unit="1" varname="bumpMap">craneArmsRotNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="craneArmsPistonSurface">
        <texture unit="0" varname="diffMap">columnDiffuse</texture>
        <texture unit="1" varname="bumpMap">craneArmsPistonNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
    <material name="subjectSurface">
        <texture unit="0" varname="diffMap">columnDiffuse</texture>
        <texture unit="1" varname="bumpMap">boxNormal</texture>
        <uniform name="useBumpMap" type="int">
            <value>1</value>
        </uniform>
    </material>
</compositor>
