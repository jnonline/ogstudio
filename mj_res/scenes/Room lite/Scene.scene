<?xml version="1.0" ?>
<Scene>
    <SkyBox Material="Scene/SkyBox" />
    <Ambient r="0.5" g="0.5" b="0.5" />
    <Light Name="Light">
        <Diffuse  r="0.6"   g="0.6" b="0.6" />
        <Specular r="0.3" g="0.3" b="0.3" />
        <Position x="-50" y="100" z="100" />
    </Light>
    <Node Name="Table">
        <Entity Name="Table" Material="Scene" Mesh="Table.mesh" CastShadows="true" />
    </Node>
	<CameraTrack Name="Start" Interpolation = "Linear">
		<Node>
            <Position x="0" y="0" z="200" />
			<Rotation x="50" y="0" z="0" /> 
        </Node>
        <Node Time="1500">
            <Position x="0" y="0" z="0" />
        </Node>
    </CameraTrack>
	<CameraTrack Name="Pause" Interpolation = "Linear">
        <Node>
            <Position x="0" y="0" z="0" />
        </Node>
        <Node Time="500">
            <Position x="0" y="-50" z="0" />
            <Rotation x="70" y="0" z="0" />
        </Node>
    </CameraTrack>
    <CameraTrack Name="Unpause" Interpolation = "Linear">
        <Node Time="0">
            <Position x="0" y="-50" z="0" />
            <Rotation x="70" y="0" z="0" />
        </Node>
        <Node Time="500">
            <Position x="0" y="0" z="0" />
        </Node>
    </CameraTrack>
</Scene>
